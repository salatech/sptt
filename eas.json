{
	"cli": {
		"version": ">= 13.1.1",
		"appVersionSource": "remote"
	},
	"build": {
		"development": {
			"developmentClient": true,
			"distribution": "internal"
		},
		"preview": {
			"distribution": "internal",
			"android": {
				"buildType": "apk"
			}
		},
		"preview2": {
			"android": {
				"gradleCommand": ":app:assembleRelease"
			}
		},
		"preview3": {
			"developmentClient": true
		},
		"preview4": {
			"distribution": "internal"
		},
		"production": {
			"ios": {
				"simulator": false,
				"distribution": "store"
			},
			"android": {
				"buildType": "app-bundle"
			},
			"autoIncrement": true
		}
	},
	"submit": {
		"production": {}
	}
}
